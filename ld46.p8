--obj
function mobj(x,y,w,h,col,ovlp,dis)
	local t={
		col=col,
		ovlp=ovlp,
		dis=dis
	}
	
	return merge_t(t,rectangle(x,y,w,h))	
end

function mob(x,y,w,h,sx,sy)
	local t,mt
	
	t={
		mob=true,
		sx=sx,
		sy=sy,
		dx=0,
		dy=0,
		canm=0,
		cld=false,
		flp=false,
		eca=0,
		mv=function(s)
			s.eca=rectangle(x-8,y-8,24,24)
			s.cld=false
			
			local hx=hit(s.x+s.dx,s.y,s.w-s.sx,s.h-s.sy,0)
			local hy=hit(s.x,s.y+s.dy,s.w-s.sx,s.h-s.sy,0)
				
			if #hx>0 then
				s.dx=0
				s.cld=true
			end
				
			if #hy>0 then
				s.dy=0
				s.cld=true
			end
				
			s.x+=s.dx
			s.y+=s.dy
		end
	}
	
	mt=merge_t(t,obj(x,y,w,h,true))
	mt.ovlp=false
	return mt
end

function player(x,y)
	local t={
		plyr=true,
		
		anms={
			--sprite,frame,speed
			{52,4,3},
			{52,2,20}
		},
		
		inv={
			{1,2},
			{2,1},
			{3,3},
			{4,1},
			{7,1},
		},
		
		bbs=2,
		bx=2,
		by=0,
		
		mvu=function(s)
			if _s.msts then
				local mvd=false
		
				s.dx=0
				s.dy=0
			
				if btn(0) then 
					s.dx=-s.sx
					s.flp=true
					mvd=true
					s.bx=-s.bbs
					s.by=0
				end
				if btn(1) then 
					s.dx=s.sx
					s.flp=false
					mvd=true
					s.bx=s.bbs
					s.by=0
				end
				if btn(2) then 
					s.dy=-s.sy
					mvd=true
					s.bx=0
					s.by=-s.bbs
				end
				if btn(3) then 
					s.dy=s.sy
					mvd=true
					s.bx=0
					s.by=s.bbs
				end
				if btnp(5) then
					add(_s.pblts,
					bullet(s.x,s.y,s.bx,s.by,_s.pblts))
				end
				
				if mvd then
					s:mv()
					s.canm=s.anms[1]
				else
					s.canm=s.anms[2]
				end
				
				s:chckintr()
			end
		end,
	
		u=function(s)
			s:mvu()
			ucam(s.x,s.y)
			if btnp(4) then
				if _s.inv then
					closeinv()
				else
					openinv(s.inv)
				end
			end
			
--			if _s.inv then
--				if btnp(2) then
--					if _s.invi>1 then
--						_s.invi-=1
--					end
--				end
--				
--				if btnp(3) then
--					if _s.invi<#_s.invb then
--						_s.invi+=1
--					end
--				end
--			end
		end,
		
		chckintr=function(s)			
			local x,y=flr(s.x/8),flr(s.y/8)
			
			for y=y-1,y+1 do
				for x=x-1,x+1 do
					if (getflg(x,y,2)) then
						s:dointr(mget(x,y),x,y)
					end
				end
			end
		end,
		
		dointr=function(s,id,x,y)
			_s.tltp={x=x,y=y}
			if id==71 then
				_s.atxt=txts.rf
			end
			
			if fget(id,7) then
				_s.atxt=txts.rme
				
				if btnp(5) then
					enterroom(x,y)
				end
			end
			
			if fget(id,6) then
				_s.atxt=txts.rmq
				if btnp(5) then
					quitroom()
				end
			end
		end,
		
		d=function(s)
			spr(
			s.canm[1]+((_s.t/s.canm[3])%s.canm[2]),
			s.x,s.y,1,1,s.flp)
		end
	}
	
	return merge_t(t,mob(x,y,8,8,1,1))
end

function bullet(x,y,sx,sy,prt)
	local t,mt
	t={
		x=x,
		y=y,
		bullet=true,
		alv=true,
		flpx=false,
		flpy=false,
		frm=7,
		
		mvu=function(s)
			s.dx=s.sx
			s.dy=s.sy
		
			s:mv()
			if s.cld then
				del(prt,s)
			end
			
			if s.dx!=0 then
				s.frm=7
				if s.dx<0 then
					s.flpx=true
				else
					s.flpx=false
				end
			elseif dy!=0 then
				s.frm=23
				if s.dy<0 then
					s.flpy=false
				else
					s.flpy=true
				end
			end
			
		end,
		
		u=function(s)
			s:mvu()
		end,
		
		d=function(s)
			spr(
			s.frm+((_s.t/10)%2),
			s.x,s.y,1,1,s.flpx,s.flpy)
		end
	}
	
	mt=merge_t(t,mob(x,y,6,6,sx,sy))
	return mt
end
